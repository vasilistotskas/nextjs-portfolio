{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "cleanUrls": true,
    "headers": [
        {
            "source": "/fonts/ibm-plex-sans-var.woff2",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/fonts/ibm-plex-sans-var-italic.woff2",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/vasilistotskas.pdf",
            "destination": "/vasilistotskas-resume.pdf"
        },
        {
            "source": "/vasilistotskas-resume-2019.pdf",
            "destination": "https://read.cv/vasilistotskas"
        },
        {
            "source": "/resume",
            "destination": "https://read.cv/vasilistotskas"
        },
        {
            "source": "/projects",
            "destination": "/about"
        }
    ]
}
